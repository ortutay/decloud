package msg

import(
	"testing"
)

func TestEncodeDecodeOcReq(t *testing.T) {
	req := OcReq{
		Service: "testService",
		Method: "testMethod",
		Args: []string{"1", "2"},
		NodeId: []{"id1", "id2", "id3"},
		Sig: []{"sig1", "sig2", "sig3"},
		Nonce: "abcnonce",
		PaymentType: ATTACHED,
		PaymentValue: &{Amount: 1e8, Currency: BTC},
		PaymentTxn: "base64-btc-txn",
		Body: []byte(),
	}
}
